<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/color_white"
    android:orientation="vertical"
    android:paddingTop="@dimen/spacing_left_reight">

    <include layout="@layout/include_btn_my_homing" />


    <android.support.v7.widget.CardView
        android:id="@+id/cvLine"
        android:layout_width="match_parent"
        android:layout_height="161dp"
        android:layout_margin="@dimen/normal_safe_distance"
        android:layout_marginTop="@dimen/normal_safe_distance"
        app:cardCornerRadius="3dp"
        app:cardElevation="5dp">


        <com.github.mikephil.charting.charts.LineChart
            android:id="@+id/line"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />

    </android.support.v7.widget.CardView>
    
    <View
        android:background="#f5f5f5"
        android:layout_width="match_parent"
        android:layout_height="10dp"/>

</LinearLayout>